{
	// See https://go.microsoft.com/fwlink/?LinkId=733558
	// for the documentation about the tasks.json format
	"version": "2.0.0",
	"tasks": [
		{
			"label": "move assets",
			"type": "shell",
			"command": "${workspaceFolder}/.vscode/move_assets.windows.cmd",
			"group": "build"
		},
		{
			"label": "move src",
			"type": "shell",
			"command": "${workspaceFolder}/.vscode/move_src.windows.cmd",
			"group": "build"
		},
		{
			"label": "move",
			"dependsOn":[
				"move src",
				"move assets",
			],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		}
	]
}